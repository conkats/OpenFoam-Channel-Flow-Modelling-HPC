FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      yPlus;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    bottom
    {
        type            calculated;
        value           uniform 2.66837;
    }
    top
    {
        type            calculated;
        value           uniform 2.66837;
    }
    front
    {
        type            cyclic;
    }
    Back
    {
        type            cyclic;
    }
}


